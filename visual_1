-- Whoop metrics as shown in the first visual here on this dashboard: 
-- https://public.tableau.com/app/profile/jarrad.stella/viz/WhoopFitnessDashboard/Dashboard2#1

SELECT
    COUNT(*) AS total_workouts,
    ROUND(AVG(calories_burned), 2) AS avg_calories_burned,
    SUM(calories_burned) AS total_calories_burned,
    MAX(max_hr) AS max_heartrate
FROM 
    'whoop_workouts.csv';
