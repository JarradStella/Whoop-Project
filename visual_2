-- Whoop metrics as shown in the second visual here on this dashboard: 
-- https://public.tableau.com/app/profile/jarrad.stella/viz/WhoopFitnessDashboard/Dashboard2#1

SELECT
    activity_name,
    COUNT(*) AS count_activities
FROM 
    'whoop_workouts.csv'
GROUP BY 
    activity_name
ORDER BY 
    count_activities DESC;
